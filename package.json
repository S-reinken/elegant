{
  "name": "elegant",
  "version": "0.0.0",
  "description": "A beautiful open source personal finance manager",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "build-server": "tsc --build ./tsconfig.server.json -w",
    "start-server": "nodemon ./dist/server/server/index.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "productName": "Elegant",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Schuyler Reinken",
  "license": "GNU General Public License Ver. 3",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/react-dom": "^16.9.0",
    "@types/sqlite3": "^3.1.5",
    "crocks": "^0.12.4",
    "csv-parser": "^2.3.1",
    "electron-fetch": "^1.3.0",
    "express": "^4.17.1",
    "fp-ts": "^2.1.0",
    "module-alias": "^2.2.2",
    "monet": "^0.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sqlite3": "^4.1.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/lab": "^4.0.0-alpha.28",
    "@mdi/js": "^4.4.95",
    "@mdi/react": "^1.2.1",
    "@types/lodash": "^4.14.138",
    "@types/material-ui": "^0.21.7",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.9.2",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "acorn": "^6.4.1",
    "ajv": "^6.6.2",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "^3.1.2",
    "electron-builder": "^20.38.5",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.4",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1"
  },
  "repository": {
    "type": "git"
  },
  "_moduleAliases": {
    "@common": "dist/server/common"
  }
}
